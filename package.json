{
	"name": "tempo",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:web": "vite dev",
		"dev:db": "prisma dev -n tempo -P 4000 -p 4001",
		"dev:schema": "prisma generate --watch",
		"build": "run-s build:prisma build:migrate build:web",
		"build:prisma": "prisma generate",
		"build:migrate": "prisma migrate deploy",
		"build:web": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"cleanup": "run-s format lint check",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.1",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.553.0",
		"@sveltejs/adapter-netlify": "^5.2.4",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/ws": "^8.18.1",
		"bits-ui": "^2.14.3",
		"clsx": "^2.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"mode-watcher": "^1.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"prisma": "^6.19.0",
		"svelte": "^5.43.6",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.6",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@clerk/localizations": "^3.27.2",
		"@inlang/paraglide-js": "^2.4.0",
		"@neondatabase/serverless": "^1.0.2",
		"@prisma/adapter-neon": "^6.19.0",
		"@prisma/client": "^6.19.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@thetinkerinc/isolocal": "^0.1.1",
		"date-fns": "^4.1.0",
		"radashi": "^12.7.0",
		"svelte-clerk": "^0.17.9",
		"valibot": "^1.1.0",
		"ws": "^8.18.3"
	}
}
