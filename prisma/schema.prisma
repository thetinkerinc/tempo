datasource db {
  provider = "postgresql"
}

generator dev {
  provider = "prisma-client"
  output = "../src/generated"
}

generator prod {
  provider = "prisma-client"
  runtime = "cloudflare"
  output = "../src/generated"
}

model Entry {
  id String @id @default(dbgenerated("gen_random_uuid()"))
  user String
  date DateTime @db.Timestamptz(3)
  hours Float
  project String?

  @@index(fields: [user])
}
